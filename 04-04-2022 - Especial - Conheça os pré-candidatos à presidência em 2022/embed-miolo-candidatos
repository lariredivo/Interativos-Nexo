<script>
  (function () {
    const listaSelecao = document.querySelectorAll('.item')
    const cards = document.querySelectorAll('.container-wrapper')
    let visibleId = 'cartao-bolsonaro'

    const toggleVisibility = cardId => {
      cardId = 'cartao-' + cardId
      if (visibleId !== cardId) {
        visibleId = cardId
      }
      hideNonVisibleCards()
    }

    const hideNonVisibleCards = () => {
      let i, cardId
      for (i = 0; i < cards.length; i++) {
        cardId = cards[i].id
        if (cardId === visibleId) {
          document.getElementById(cardId).style.display = 'block'
        } else {
          document.getElementById(cardId).style.display = 'none'
        }
      }
    }

    listaSelecao.forEach(card => {
      card.addEventListener('click', () => {
        //remover o cartao aberto
        toggleVisibility(card.id)
      })
    })
  })()
</script>

</html>
